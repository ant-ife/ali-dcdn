{
  "name": "ali-dcdn",
  "version": "1.4.1",
  "description": "Aliyun DCDN client",
  "keywords": [
    "ali",
    "aliyun",
    "dcdn",
    "sdk",
    "client",
    "cdn"
  ],
  "main": "lib",
  "dependencies": {
    "urllib": "^2.36.1"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "babel-eslint": "^10.1.0",
    "debug": "^4.3.1",
    "eslint": "^7.14.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "sinon": "^9.2.1"
  },
  "optionalDependencies": {
    "standard-version": "^9.0.0"
  },
  "scripts": {
    "build-change-log": "standard-version",
    "lint": "eslint . --fix",
    "cov": "nyc report --reporter=text-lcov",
    "ci": "npm run lint && npm run test && npm run cov",
    "test": "nyc mocha -R spec test/*.test.js"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "include": [
      "lib",
      "index.js"
    ],
    "reporter": [
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ant-ife/ali-dcdn.git"
  }
}
